library(dplyr)
library(tidyverse)

##Read MechaCar_mpg.csv and perform linear regression
MechaCar <- read.csv(file="MechaCar_mpg.csv")
reg <- lm(mpg ~ vehicle_length + vehicle_weight + spoiler_angle + ground_clearance + AWD, data = MechaCar)
summary(reg)


